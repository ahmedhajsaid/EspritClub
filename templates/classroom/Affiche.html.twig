<body>
<h1>Liste des classrooms</h1>
<table border="1">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Modifier</th>
        <th>Supprimer</th>
    </tr>
    <tr {% for c in classroom %} >
            <td>{{ c.id }}</td>
            <td>{{ c.name }}</td>
        <td> <a href="{{ path ('update', {'id':c.id}) }}">Modifier</a> </td>
        <td> <a href="{{ path ('d', {'id':c.id}) }}">Supprimer</a> </td>
    </tr {% endfor %} >




</table>
</body>
